{
  "name": "@instant-api/recipe-book",
  "version": "1.2.0",
  "description": "> A CLI to create a recipe book API",
  "homepage": "https://github.com/instant-api/recipe-book#readme",
  "bugs": {
    "url": "https://github.com/instant-api/recipe-book/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instant-api/recipe-book.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc",
  "main": "dist/index.js",
  "module": "dist/recipe-book.esm.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "recipe-book": "bin/index.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "api.html"
  ],
  "scripts": {
    "build": "tsdx build",
    "lint": "tsdx lint",
    "pub": "tsdx build && np",
    "run": "npx -p . recipe-book",
    "start": "tsdx watch",
    "test": "tsdx test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "arg": "^5.0.0",
    "cuid": "^2.1.8",
    "fs-extra": "^9.0.1",
    "get-port": "^5.1.1",
    "inquirer": "^7.3.3",
    "tumau": "^1.0.0-alpha.84",
    "zod": "^1.11.10"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.4",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.15",
    "@types/yup": "^0.29.9",
    "husky": "^4.3.0",
    "np": "^7.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public"
  }
}
